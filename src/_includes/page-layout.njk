



{% set meta_title = meta_title or config.year + ' ' + title + ' :: Vermont Code Camp' %}
{% set meta_description = meta_description or description %}


<!doctype html>
<html class="no-js" lang="en">
<head>
    <title>{{meta_title}}</title>
    <link rel="icon" href="/favicon.ico" type="image/x-icon" />

    <meta name="description" content="{{meta_description}}">
    <meta name="viewport" content="width=device-width,initial-scale=1.0" />
    <link href="/humans.txt" rel="author" type="text/plain" />

    <link href="/css/reset.css?rev=aab9ad3fbc2c1408447c191f51ccc059bfb0cb11" rel="stylesheet" type="text/css" media="handheld, all" />
    <link href="/css/layout.css?rev=274680d81e779d6a5e485aa0417ac85b852c3ba8" rel="stylesheet" type="text/css" media="handheld, all" />
    <link href="/css/content.css?rev=d7e210bf7cecbf615c4edc9ab5041a2014bd18cd" rel="stylesheet" type="text/css" media="handheld, all" />
    
    <script src="/js/modernizr-2.6.2.min.js" type="text/javascript"></script>
    <script src="/js/jquery-1.7.1.min.js" type="text/javascript"></script>
    <script src="/js/jquery.smooth-scroll.min.js" type="text/javascript"></script>
    <script src="/js/jquery.ba-hashchange.min.js" type="text/javascript"></script>
    <script src="/js/layout.js?rev=5a06caa2c39225431cbeec861ffcc51d38933c4f" type="text/javascript"></script>
    
</head>
<body>
    <header class="page">
        <div class="inner">
            <h1 >
                <a class="header-logo" href="/">
                    {% include "vermont-code-camp-logo-2018.svg" %}
                </a>
            </h1>
        </div>
    </header>
    <aside class="page">
        <div class="inner">
            <nav>
                <ul class="list-inline">
                    <li><a href="/schedule">
                        {% include "fa/calendar-alt.svg" %} Schedule
                    </a></li>
                    <li><a href="/sessions">
                        {% include "fa/desktop.svg" %} Sessions
                    </a></li>
                    <li><a href="/speakers">
                        {% include "fa/user.svg" %} Speakers
                    </a></li>
                </ul>
            </nav>
        </div>
    </aside>

    <section id="content" class="event-2017 page content">
        {{ content | safe }}
    </section>

    <footer class="page">
        <div class="inner">
            <p>&copy; Vermont Code Camp | <a href="mailto:info@vtcodecamp.org">info@vtcodecamp.org</a> | @<a href="http://twitter.com/VTCodeCamp">VTCodeCamp</a></p>
            <p>A special thanks to <a href="http://www.flickr.com/photos/42504188@N03/">Rob Hale</a> and <a href="http://www.flickr.com/photos/daveburkevt/">Dave Burke</a> for permission to use their photography of Vermont Code Camp.</p>
        </div>
    </footer>
</body>
</html>
